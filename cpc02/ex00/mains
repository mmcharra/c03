// ex00

  #include <stdio.h>
 
  char *ft_strcpy(char *dest, char *src)
  {
      int cycle;
 
      cycle = 0;
      while (src[cycle] != '\0')
      {
          dest[cycle] = src[cycle];
          cycle++;
      }
      dest[cycle] = '\0';
      return (dest);
  }
 
 
  int main(void)
  {
      char    src[] = "If this is not my name then this is my name";
      char    des[] = "";
 
      ft_strcpy(des, src);
      printf("%s",  des);
      printf("\n");
      return (0);
  }


  // ex01


